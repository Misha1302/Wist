import "WistRandomLib.dll"
import "Array.wst"

func Start() {
    Print(new Dog().Eat())
}

class AnimalBase(_eat) {
    method Ctor() {
        this._eat = 0
    }

    method Eat() {
        this._eat += 10
    }
}

class Dog(parents) {
    method Ctor() {
        this.parents = new Array([new AnimalBase()])
    }
    
    method Eat() {
        this.parents.GetElem(1).Eat()
        return this.parents.GetElem(1)._eat
    }
}